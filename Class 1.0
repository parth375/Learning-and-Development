#include <iostream>
using namespace std;
class A{
    private:
    int a;      // declared variable in private or data members
    int b;
    public:
    A(int x, int y){      // constructor
       a=x; 
       b=y;
   }
   void show(){             // methods
    cout<<a<<" "<<b<<endl;     
   }
};
int main()
{
    A object(3,4);    // object created of class A
    object.show();   // calling method using the object
    return 0;
}
